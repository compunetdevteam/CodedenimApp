@model CodedenimWebApp.ViewModels.CreteCourseVm

@{
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";
}

<!-- // END Tabbable Widget -->
<h4 class="page-section-heading">Upload Courses</h4>
<!-- Tabbable Widget -->
<div class="tabbable tabs-primary">
    <!-- Tabs -->
    <ul class="nav nav-tabs">
        <li class="active"><a href="#color-home" data-toggle="tab"><i class="fa fa-fw fa-home"></i>Create Course</a></li>
        
    </ul>
    <!-- // END Tabs -->
    <!-- Panes -->
    <div class="tab-content">
        <div id="color-home" class="tab-pane active">
                @Html.Partial("CreateCoursePartial")
        </div>
      
    </div>
    <!-- // END Panes -->
</div>
<!-- // END Tabbable Widget -->







<script type="text/javascript">
    $(document).ready(function () {
        $('.datepicker').datepicker(
            {
                dateFormat: "dd/M/yy",
                changeMonth: true,
                changeYear: true,
                yearRange: "-60:+0"
            }
        );

    });
</script> 